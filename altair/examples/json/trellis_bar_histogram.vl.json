{
    "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
    "data": {
        "url": "http://vega.github.io/vega-lite/data/cars.json"
    },
    "encoding": {
        "row": {
            "field": "Origin",
            "type": "nominal"
        },
        "x": {
            "bin": {
                "maxbins": 15
            },
            "field": "Horsepower",
            "type": "quantitative"
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    },
    "mark": "bar"
}