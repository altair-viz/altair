{
    "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
    "data": {
        "url": "http://vega.github.io/vega-lite/data/seattle-weather.csv"
    },
    "encoding": {
        "opacity": {
            "value": 0.5
        },
        "size": {
            "field": "weather",
            "type": "nominal"
        },
        "x": {
            "field": "date",
            "timeUnit": "month",
            "type": "temporal"
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    },
    "mark": "bar"
}