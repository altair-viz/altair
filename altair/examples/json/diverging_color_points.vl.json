{
    "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
    "data": {
        "url": "http://vega.github.io/vega-lite/data/cars.json"
    },
    "description": "A bubbleplot showing horsepower on x, miles per gallons on y, and weight with a diverging color scale.",
    "encoding": {
        "color": {
            "field": "Weight_in_lbs",
            "scale": {
                "domain": [
                    1000,
                    2750,
                    4500
                ],
                "range": [
                    "#ff7f0e",
                    "#e7ba52",
                    "#17becf"
                ]
            },
            "type": "quantitative"
        },
        "x": {
            "field": "Horsepower",
            "type": "quantitative"
        },
        "y": {
            "field": "Miles_per_Gallon",
            "type": "quantitative"
        }
    },
    "mark": "point"
}