{
    "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
    "data": {
        "url": "http://vega.github.io/vega-lite/data/seattle-weather.csv"
    },
    "encoding": {
        "color": {
            "field": "weather",
            "type": "nominal"
        },
        "x": {
            "field": "date",
            "timeUnit": "month",
            "type": "temporal"
        },
        "y": {
            "aggregate": "count",
            "field": "*",
            "type": "quantitative"
        }
    },
    "mark": "bar"
}