{
    "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
    "data": {
        "url": "http://vega.github.io/vega-lite/data/stocks.csv"
    },
    "description": "Stock prices of 5 Tech Companies over Time.",
    "encoding": {
        "color": {
            "field": "symbol",
            "type": "nominal"
        },
        "x": {
            "axis": {
                "format": "%Y"
            },
            "field": "date",
            "type": "temporal"
        },
        "y": {
            "field": "price",
            "type": "quantitative"
        }
    },
    "mark": "line"
}