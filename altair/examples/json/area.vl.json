{
    "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
    "data": {
        "url": "http://vega.github.io/vega-lite/data/unemployment-across-industries.json"
    },
    "encoding": {
        "x": {
            "axis": {
                "format": "%Y",
                "labelAngle": 0
            },
            "field": "date",
            "scale": {
                "nice": "month"
            },
            "timeUnit": "yearmonth",
            "type": "temporal"
        },
        "y": {
            "aggregate": "sum",
            "field": "count",
            "type": "quantitative"
        }
    },
    "height": 200,
    "mark": "area",
    "width": 300
}