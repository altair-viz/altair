{
    "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
    "data": {
        "url": "http://vega.github.io/vega-lite/data/cars.json"
    },
    "encoding": {
        "x": {
            "field": "Name",
            "scale": {
                "round": false
            },
            "type": "nominal"
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    },
    "height": 120,
    "mark": "bar",
    "width": 120
}