{
    "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
    "data": {
        "url": "http://vega.github.io/vega-lite/data/cars.json"
    },
    "description": "Area chart showing weight of cars over time (vertical).",
    "encoding": {
        "x": {
            "aggregate": "sum",
            "field": "Weight_in_lbs",
            "type": "quantitative"
        },
        "y": {
            "field": "Year",
            "timeUnit": "year",
            "type": "temporal"
        }
    },
    "mark": {
        "interpolate": "monotone",
        "type": "area"
    }
}